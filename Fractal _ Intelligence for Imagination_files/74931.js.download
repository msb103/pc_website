        if (jQuery('.home-banner-wrapper').length > 0) {
            // Add new class to <body> element
            jQuery('body').addClass('home-banner-section-template');
            jQuery('.home-banner-wrapper').addClass('home-banner-for-fullwidth-emplate');
			 jQuery('.banner-container').addClass('banner-auto-height');
        }
    }
	
	
});
